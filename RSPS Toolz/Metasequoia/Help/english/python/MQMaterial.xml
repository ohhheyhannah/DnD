  <method>
   <name>allocUserData</name>
   <message lang="en">Allocate a memory area for an user data in the material.</message>
   <message lang="ja">材質にユーザーデータ用の領域を確保します。</message>
   <args>
    <arg>
     <name>userdata_id</name>
     <type>int</type>
     <message lang="en">A material user data</message>
     <message lang="ja">材質ユーザーデータID</message>
    </arg>
   </args>
   <return>
    <type>int</type>
    <message lang="en">Result</message>
    <message lang="ja">結果</message>
    <values>
     <value value="0">
      <message lang="en">failed</message>
      <message lang="ja">失敗</message>
     </value>
     <value value="1">
      <message lang="en">succeeded</message>
      <message lang="ja">成功</message>
     </value>
    </values>
   </return>
  </method>
