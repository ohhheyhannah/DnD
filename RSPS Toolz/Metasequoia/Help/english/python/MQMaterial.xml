   <get>1</get>
   <set>1</set>
   <type>string</type>
   <name>name</name>
   <message lang="en">material's name</message>
   <message lang="ja">マテリアル名</message>
  </property>
  <property>
   <get>1</get>
   <set>0</set>
   <type>long</type>
   <name>id</name>
